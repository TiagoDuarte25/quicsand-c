<?xml version="1.0" ?>
<experiment boot="kollaps:2.0">
    <services>
        <service name="dashboard" image="kollaps/dashboard:1.0" supervisor="true" port="8088"/>
        <service name="client1" image="quicsand" command="['server','1']"/>
        <service name="client2" image="quicsand" command="['server','1']"/>
        <service name="client3" image="quicsand" command="['server','1']"/>
        <service name="client4" image="quicsand" command="['server','1']"/>
